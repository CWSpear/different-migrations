{
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/surgeforward/phinx.git"
        }
    ],
    "require": {
        "robmorgan/phinx": "dev-schema-diff-ext-patch",
        "symfony/console": "~2.6.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "phpspec/nyan-formatters": "1.*"
    },
    "autoload": {
        "psr-4": {
            "CWSpear\\": "src/CWSpear"
        },
        "psr-0": {
            "": "src"
        }
    },
    "minimum-stability": "dev"
}
